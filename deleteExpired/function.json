{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 0 */3 * *",
      "runOnStartup": false
    },
    {
      "type": "cosmosDB",
      "name": "expiredEvents",
      "databaseName": "deer-bot-db",
      "collectionName": "Events",
      "connectionStringSetting": "COSMOS_CONNECTION_STRING",
      "direction": "in",
      "sqlQuery": "SELECT * FROM e WHERE e.date < GetCurrentDateTime()"
    }

  ]
}
